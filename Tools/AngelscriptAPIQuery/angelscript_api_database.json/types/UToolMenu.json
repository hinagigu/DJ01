{
  "name": "UToolMenu",
  "inherits": "UToolMenuBase",
  "doc": "Represents a menu in the ToolMenus system.\n\nAn instance of this class is returned by basic APIs such as UToolMenus::RegisterMenu and UToolMenus::ExtendMenu.",
  "properties": {
    "MenuName": [
      "FName"
    ],
    "MenuParent": [
      "FName"
    ],
    "StyleName": [
      "FName"
    ],
