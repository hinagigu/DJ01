      "FName"
    ],
    "MenuType": [
      "EMultiBoxType"
    ],
    "bShouldCloseWindowAfterMenuSelection": [
      "bool"
    ],
    "bCloseSelfOnly": [
      "bool"
    ],
    "bSearchable": [
      "bool"
    ],
    "bToolBarIsFocusable": [
      "bool"
    ],
    "bToolBarForceSmallIcons": [
      "bool"
