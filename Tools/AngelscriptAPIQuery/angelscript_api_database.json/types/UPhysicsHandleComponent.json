{
  "name": "UPhysicsHandleComponent",
  "inherits": "UActorComponent",
  "doc": "Utility object for moving physics objects around.",
  "properties": {
    "bSoftAngularConstraint": [
      "bool"
    ],
    "bSoftLinearConstraint": [
      "bool"
    ],
    "bInterpolateTarget": [
      "bool"
    ],
    "LinearDamping": [
      "float32",
      "Linear damping of the handle spring."
    ],
    "LinearStiffness": [
      "float32",
      "Linear stiffness of the handle spring"
    ],
    "AngularDamping": [
      "float32",
      "Angular damping of the handle spring"
    ],
    "AngularStiffness": [
      "float32",
      "Angular stiffness of the handle spring"
    ],
    "InterpolationSpeed": [
      "float32",
      "How quickly we interpolate the physics target transform"
    ]
  },
  "methods": [
    {
      "name": "GetGrabbedComponent",
      "return": "UPrimitiveComponent",
      "const": true,
      "doc": "Returns the currently grabbed component, or null if nothing is grabbed.",
      "ufunction": true,
      "meta": null,
      "args": []
    },
    {
      "name": "GetTargetLocationAndRotation",
      "return": "void",
      "const": true,
      "doc": "Get the current location and rotation",
      "ufunction": true,
      "meta": null,
      "args": [
        {
          "name": "TargetLocation",
          "type": "FVector&"
        },
        {
          "name": "TargetRotation",
          "type": "FRotator&"
        }
      ]
    },
    {
      "name": "GrabComponentAtLocation",
      "return": "void",
      "doc": "Grab the specified component at a given location. Does NOT constraint rotation which means the handle will pivot about GrabLocation.",
      "ufunction": true,
      "meta": null,
      "args": [
        {
          "name": "Component",
          "type": "UPrimitiveComponent"
        },
        {
          "name": "InBoneName",
          "type": "FName"
        },
        {
          "name": "GrabLocation",
          "type": "FVector"
        }
      ]
    },
    {
      "name": "GrabComponentAtLocationWithRotation",
      "return": "void",
      "doc": "Grab the specified component at a given location and rotation. Constrains rotation.",
      "ufunction": true,
      "meta": null,
      "args": [
        {
          "name": "Component",
          "type": "UPrimitiveComponent"
        },
        {
          "name": "InBoneName",
          "type": "FName"
        },
        {
          "name": "Location",
          "type": "FVector"
        },
        {
          "name": "Rotation",
          "type": "FRotator"
        }
      ]
    },
    {
      "name": "ReleaseComponent",
      "return": "void",
