      "meta": null,
      "args": [
        {
          "name": "TabNameID",
          "type": "FName"
        },
        {
          "name": "TabButton",
          "type": "UCommonButtonBase"
        }
      ]
    },
    {
      "name": "HandleTabRemoval",
      "return": "void",
      "protected": true,
      "callable": false,
      "event": true,
