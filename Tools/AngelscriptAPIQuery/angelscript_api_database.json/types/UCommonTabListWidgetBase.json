{
  "name": "UCommonTabListWidgetBase",
  "inherits": "UCommonUserWidget",
  "doc": "Base class for a list of selectable tabs that correspondingly activate and display an arbitrary widget in a linked switcher",
  "properties": {
    "OnTabSelected": [
      "FOnTabSelected__CommonTabListWidgetBase",
      "Broadcasts when a new tab is selected."
    ],
    "OnTabButtonCreation": [
      "FOnTabButtonCreation__CommonTabListWidgetBase",
      "Broadcasts when a new tab is created."
    ],
    "OnTabButtonRemoval": [
      "FOnTabButtonRemoval__CommonTabListWidgetBase",
      "Broadcasts when a new tab is created."
    ],
    "OnTabListRebuilt": [
      "FOnTabListRebuilt__CommonTabListWidgetBase",
      "Broadcasts when the tab list has been rebuilt (after a new tab has been inserted rather than added to the end)."
    ],
    "NextTabInputActionData": [
      "FDataTableRowHandle",
      "The input action to listen for causing the next tab to be selected"
    ],
    "PreviousTabInputActionData": [
      "FDataTableRowHandle",
      "The input action to listen for causing the previous tab to be selected"
    ],
    "NextTabEnhancedInputAction": [
      "UInputAction",
      "The input action to listen for causing the next tab to be selected"
    ],
    "PreviousTabEnhancedInputAction": [
      "UInputAction",
      "The input action to listen for causing the previous tab to be selected"
    ],
    "bAutoListenForInput": [
      "bool",
      "Whether to register to handle tab list input immediately upon construction"
    ],
    "bDeferRebuildingTabList": [
      "bool",
      2,
      "Whether to defer until next tick rebuilding tab list when inserting new tab (rather than adding to the end).\nUseful if inserting multiple tabs in the same tick as the tab list will only be rebuilt once."
    ]
  },
  "methods": [
    {
