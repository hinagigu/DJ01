      "return": "void",
      "doc": "Disables the tab associated with the given ID with a reason",
      "ufunction": true,
      "meta": null,
      "args": [
        {
          "name": "TabNameID",
          "type": "FName"
        },
        {
          "name": "Reason",
          "type": "FText"
        }
      ]
    },
    {
      "name": "GetActiveTab",
      "return": "FName",
      "const": true,
      "doc": "@return The currently active (selected) tab",
      "ufunction": true,
      "meta": null,
      "args": []
    },
    {
      "name": "GetLinkedSwitcher",
      "return": "UCommonAnimatedSwitcher",
