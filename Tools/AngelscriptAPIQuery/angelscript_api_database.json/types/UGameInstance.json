        }
      ]
    },
    {
      "name": "GetFirstLocalPlayerController",
      "return": "APlayerController",
      "const": true,
      "args": [
        {
          "name": "World",
          "default": "nullptr",
          "type": "const UWorld"
        }
      ]
    },
    {
      "name": "FindLocalPlayerFromControllerId",
      "return": "ULocalPlayer",
